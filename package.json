{
  "name": "da-api",
  "author": "livzon da team",
  "private": true,
  "version": "1.0.0",
  "description": "DA API",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:online": "node dist/main",
    "start:local": "cross-env TASK_ENABLED=false NODE_ENV=local SERVICE_NAME=da-api CONFIG_FOLDER=../config PORT=3002 nest start --watch",
    "start:test": "pm2 start --env test --only da-api",
    "start:dev": "pm2 start --env dev --only da-api",
    "start:prod": "pm2 start --env prod --only da-api",
    "flush": "pm2 flush",
    "reload": "git pull && nest build && pm2 reload da-api"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.10",
    "@alicloud/viapi-utils": "^1.0.0",
    "@aliyunmq/mq-http-sdk": "^1.0.4",
    "@grpc/grpc-js": "^1.3.4",
    "@grpc/proto-loader": "^0.6.4",
    "@nestjs/axios": "0.0.3",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "8.0.2",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@node-rs/jieba": "^1.5.5",
    "ali-oss": "^6.16.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.15.0",
    "bull-board": "^2.1.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cls-hooked": "^4.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "exceljs": "^4.3.0",
    "ioredis": "^4",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^3.7.3",
    "mongoose": "^6.0.12",
    "mysql": "^2.18.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.27.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io-client": "^2.4.0",
    "source-map-support": "^0.5.19",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.1.6",
    "tnwx": "^2.5.6",
    "typeorm": "^0.2.41",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/ali-oss": "^6.16.2",
    "@types/bull": "^3.15.5",
    "@types/cls-hooked": "^4.3.3",
    "@types/debug": "^4.1.7",
    "@types/ioredis": "*",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.7.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "*",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.1",
    "prettier": "^2.3.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
