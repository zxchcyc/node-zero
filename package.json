{
  "name": "node-zero",
  "author": "archer.zheng",
  "private": true,
  "version": "1.0.0",
  "description": "EDC 数仓",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:online": "node dist/main",
    "start:local": "cross-env TASK_ENABLED=true NODE_ENV=local SERVICE_NAME=node-zero CONFIG_FOLDER=../config PORT=3001 nest start --watch",
    "start:dev": "nest build && pm2 start --env dev --only node-zero",
    "flush": "pm2 flush",
    "reload": "git pull && nest build && pm2 reload node-zero"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.4",
    "@grpc/proto-loader": "^0.6.4",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/microservices": "8.0.2",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/typeorm": "^8.0.2",
    "bull": "^3.15.0",
    "bull-board": "^2.1.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cls-hooked": "^4.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.3",
    "mongoose": "^6.0.12",
    "mysql": "^2.18.1",
    "nestjs-redis": "git+https://github.com/GyanendroKh/nestjs-redis.git#nest8-fix",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/testing": "^8.0.6",
    "@types/bull": "^3.15.5",
    "@types/cls-hooked": "^4.3.3",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.7.1",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.1",
    "prettier": "^2.3.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
